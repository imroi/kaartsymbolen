{
  "name": "",
  "rules": [
    {
      "name": "Opstelplaats",
      "symbolizers": [
        {
          "kind": "Icon",
          "size": 14,
          "image": "https://imroi.github.io/kaartsymbolen/iconen/voertuig/{voertuig_id}.svg",
          "rotate": { "args": ["rotatie"], "name": "property" },
          "sizeUnit": "m",
          "allowOverlap": true,
          "defaultImage": "https://imroi.github.io/kaartsymbolen/iconen/voertuig/101.svg"
        }
      ],
      "scaleDenominator": { "max": 25000 }
    },
    {
      "name": "Label",
      "symbolizers": [
        {
          "kind": "Text",
          "size": 16,
          "color": "#000000",
          "label": { "args": ["label"], "name": "property" },
          "offset": [0, 2],
          "haloColor": "#ffffff",
          "haloWidth": 1.5
        }
      ],
      "scaleDenominator": { "max": 12500 }
    }
  ]
}

